<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group" >
    <label for="titleofBlog">Title</label>
    <input type="text" class="form-control" id="title" aria-describedby="blogTitle" name="title" placeholder="">
  </div>
  <div class="form-group">
    <label for="contentofBlog">Blog</label>
    <textarea class="form-control" id="blog" rows="5" cols="30" name="blog"></textarea>
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>
<%- include("partials/footer") -%>
